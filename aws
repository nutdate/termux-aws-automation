INSTANCE_ID=$(aws ec2 run-instances \
  --image-id "$IMAGE_ID" \
  --count 1 \
  --instance-type "$INSTANCE_TYPE" \
  --key-name "$KEY_NAME" \
  --network-interfaces "[{\"SubnetId\":\"$SUBNET_ID\",\"DeviceIndex\":0,\"AssociatePublicIpAddress\":true,\"Ipv6Addresses\":[{\"Ipv6Address\":\"2406:da18:a5d:f301::1234\"}],\"Groups\":[\"$SECURITY_GROUP_ID\"]}]" \
  --block-device-mappings "DeviceName=/dev/sda1,Ebs={VolumeSize=30,DeleteOnTermination=true}" \
  --tag-specifications 'ResourceType=instance,Tags=[{Key=Name,Value=My-FreeTier-EC2-Instance}]' \
  --query "Instances[0].InstanceId" --output text)
